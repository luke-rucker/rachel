{
  "name": "rachel",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "prepare": "husky install",
    "scrape:amazon": "ts-node scripts/scrape-amazon.ts",
    "scrape:target": "ts-node scripts/scrape-target.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@remix-run/express": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "cheerio": "^1.0.0-rc.11",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "puppeteer": "^14.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
